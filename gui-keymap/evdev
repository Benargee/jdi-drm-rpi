xkb_symbols "basic" {
    name[Group1]= "English (US)";
    key <TLDE> {        [     numbersign,       asciitilde      ]       };
    key <AE01> {        [         1,    exclam          ]       };
    key <AE02> {        [         2,    at              ]       };
    key <AE03> {        [         3,    numbersign      ]       };
