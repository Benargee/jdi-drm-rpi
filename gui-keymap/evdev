    key <BKSL>{[semicolon]};

    key <PRSC>{[apostrophe]};
    key <SCLK>{[quotedbl]};

    key <PAUS>{[question]};
    key <NMLK>{[exclam]};
    key <LFSH>{[dollar,F8,F9]};
    key <AB08>{[comma]};
    key <AB09>{[period]};

    key <AD01> {        [         q,    Q ,asciitilde           ]       };
    key <AD02> {        [         w,    W,grave         ]       };
    key <AD03> {        [         e,    E,braceleft             ]       };
