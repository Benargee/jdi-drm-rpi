        <I706> = 706;           // #define KEY_KBD_LCD_MENU3       698
        <I707> = 707;           // #define KEY_KBD_LCD_MENU4       699
        <I708> = 708;           // #define KEY_KBD_LCD_MENU5       700
        // End of autogenerated key codes

        // Fake keycodes for virtual keys
        <LVL3> =   92;
